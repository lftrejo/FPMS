<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="fill_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.se2.team3.fpms.CustomRoutes"
    android:id="@+id/hey">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Name"
        android:id="@+id/textView12"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="43dp"
        android:textSize="20px" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Departure Point"
        android:id="@+id/textView13"
        android:layout_marginTop="22dp"
        android:textSize="20px"
        android:layout_below="@+id/textView16"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Waypoint 1#"
        android:id="@+id/textView14"
        android:layout_marginTop="23dp"
        android:layout_below="@+id/textView13"
        android:layout_alignParentStart="true"
        android:textSize="20px" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Destination"
        android:id="@+id/textView15"
        android:textSize="20px"
        android:layout_marginTop="28dp"
        android:layout_below="@+id/button5"
        android:layout_alignParentStart="true" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="75px"
        android:layout_height="60px"
        android:text="add"
        android:id="@+id/button5"
        android:layout_below="@+id/textView14"
        android:layout_alignParentStart="false"
        android:textSize="12px" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="75px"
        android:layout_height="60px"
        android:text="Del"
        android:id="@+id/button6"
        android:layout_alignTop="@+id/button5"
        android:layout_toEndOf="@+id/button5"
        android:textSize="12px" />

    <TextView
        android:layout_width="50px"
        android:layout_height="40px"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Altitude"
        android:id="@+id/textView16"
        android:layout_below="@+id/textView12"
        android:layout_centerHorizontal="true"
        android:textSize="14px" />

    <TextView
        android:layout_width="50px"
        android:layout_height="40px"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Longitude"
        android:id="@+id/textView17"
        android:layout_alignTop="@+id/textView16"
        android:layout_toStartOf="@+id/textView18"
        android:layout_marginEnd="28dp"
        android:textSize="14px" />

    <TextView
        android:layout_width="50px"
        android:layout_height="40px"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="True course"
        android:id="@+id/textView18"
        android:layout_alignTop="@+id/textView17"
        android:layout_toStartOf="@+id/textView19"
        android:layout_marginEnd="26dp"
        android:textSize="14px" />

    <TextView
        android:layout_width="50px"
        android:layout_height="40px"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Distance"
        android:id="@+id/textView19"
        android:layout_alignTop="@+id/textView18"
        android:layout_alignParentEnd="true"
        android:textSize="14px" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/editText2"
        android:layout_alignStart="@+id/textView16"
        android:layout_above="@+id/textView14"
        android:layout_below="@+id/textView16"
        android:layout_toStartOf="@+id/editText3" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/editText3"
        android:layout_alignBottom="@+id/editText2"
        android:layout_alignStart="@+id/textView17"
        android:layout_alignTop="@+id/editText2"
        android:layout_toStartOf="@+id/editText4" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/editText4"
        android:layout_alignBottom="@+id/editText3"
        android:layout_alignTop="@+id/editText3"
        android:layout_alignEnd="@+id/textView18"
        android:layout_toEndOf="@+id/textView17" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/editText5"
        android:layout_alignBottom="@+id/editText4"
        android:layout_toEndOf="@+id/textView18" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/editText6"
        android:layout_above="@+id/button6"
        android:layout_alignStart="@+id/editText2"
        android:layout_toStartOf="@+id/editText3" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/editText7"
        android:layout_above="@+id/button6"
        android:layout_alignStart="@+id/editText3"
        android:layout_toStartOf="@+id/editText4" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/editText8"
        android:layout_alignBottom="@+id/editText7"
        android:layout_alignStart="@+id/editText4"
        android:layout_toStartOf="@+id/editText5" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/editText9"
        android:layout_above="@+id/button6"
        android:layout_alignStart="@+id/editText5" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/editText10"
        android:layout_alignBottom="@+id/textView15"
        android:layout_alignStart="@+id/editText6"
        android:layout_toStartOf="@+id/editText11" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/editText11"
        android:layout_alignBottom="@+id/editText10"
        android:layout_alignStart="@+id/editText7"
        android:layout_toStartOf="@+id/editText12" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/editText12"
        android:layout_alignBottom="@+id/editText11"
        android:layout_alignStart="@+id/editText8"
        android:layout_toStartOf="@+id/editText13" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/editText13"
        android:layout_alignBottom="@+id/editText12"
        android:layout_toEndOf="@+id/textView18" />

    <EditText
        android:layout_width="150px"
        android:layout_height="wrap_content"
        android:id="@+id/editText14"
        android:layout_above="@+id/textView16"
        android:layout_toEndOf="@+id/button6" />

    <EditText
        android:layout_width="150px"
        android:layout_height="wrap_content"
        android:id="@+id/editText15"
        android:layout_alignBottom="@+id/editText2"
        android:layout_alignStart="@+id/editText16" />

    <EditText
        android:layout_width="150px"
        android:layout_height="wrap_content"
        android:id="@+id/editText16"
        android:layout_alignBottom="@+id/editText6"
        android:layout_toEndOf="@+id/button6" />

    <EditText
        android:layout_width="150px"
        android:layout_height="wrap_content"
        android:id="@+id/editText17"
        android:layout_alignBottom="@+id/editText10"
        android:layout_toEndOf="@+id/button6" />

</RelativeLayout>
